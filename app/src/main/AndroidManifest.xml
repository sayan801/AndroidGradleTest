<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.mycuratioui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="com.app.mycuratioui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.mycuratioui.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.app.mycuratioui.SignupActivity"
            android:label="@string/title_activity_signup" >
        </activity>
        <activity
            android:name="com.app.mycuratioui.AccountRecovery"
            android:label="@string/title_activity_account_recovery" >
        </activity>
        <activity
            android:name="com.app.mycuratioui.UserDetailsActivity"
            android:label="@string/title_activity_user_details" >
        </activity>
        <activity
            android:name="com.app.mycuratioui.UserDob"
            android:label="@string/title_activity_user_dob" >
        </activity>
        <activity
            android:name="com.app.mycuratioui.UserPasscode"
            android:label="@string/title_activity_user_passcode" >
        </activity>
        <activity
            android:name="com.app.mycuratioui.UserConfirmPasscode"
            android:label="@string/title_activity_user_confirm_passcode" >
        </activity>
    </application>

</manifest>
